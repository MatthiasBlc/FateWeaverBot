{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/server.js",
  "scripts": {
    "devstart": "nodemon src/server.ts",
    "start": "node dist/src/server.js",
    "dev": "ts-node --transpile-only -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start:migrate:prod": "prisma migrate deploy && prisma db seed && node dist/src/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:seed": "tsx --tsconfig tsconfig.json prisma/seed.ts",
    "migrate:full": "prisma migrate dev && npm run prisma:seed",
    "postinstall": "prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "prisma": {
    "seed": "sh scripts/seed-docker.sh"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.19",
    "@types/express-session": "^1.17.10",
    "@types/http-errors": "^2.0.3",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.7",
    "@types/node": "^24.8.0",
    "@types/supertest": "^6.0.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.5.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@prisma/client": "^5.5.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "@quixo3/prisma-session-store": "^3.1.13",
    "@types/cron": "^2.0.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "cron": "^4.3.3",
    "discord.js": "^14.23.2",
    "dotenv": "^16.3.1",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
